{
  "name": "murder-king",
  "version": "1.0.0",
  "description": "A multiplayer jousting game",
  "scripts": {
    "prestart": "node scripts/build-asset-pack",
    "start": "react-app-rewired start --scripts-version react-scripts-ts",
    "build": "react-app-rewired build --scripts-version react-scripts-ts",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "format": "prettier-standard 'src/**/*.ts' 'src/**/*.tsx'",
    "precommit": "lint-staged",
    "lint": "standard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cdaringe/murder-king.git"
  },
  "author": "cdaringe <cdaringe@gmail.com>",
  "private": true,
  "license": "PRIVATE",
  "bugs": {
    "url": "https://github.com/cdaringe/murder-king/issues"
  },
  "homepage": "https://github.com/cdaringe/murder-king#readme",
  "devDependencies": {
    "@types/node": "^10.9.1",
    "@types/react": "^16.4.11",
    "@types/react-dom": "^16.0.7",
    "klaw-sync": "^4.0.0",
    "lint-staged": "^7.2.2",
    "npm-run-all": "^4.1.3",
    "phaser": "^3.11.0",
    "phaser-webpack-loader": "^2.0.0",
    "prettier-standard": "^8.0.1",
    "react": "^16.4.2",
    "react-app-rewired": "^1.5.2",
    "react-dom": "^16.4.2",
    "react-scripts-ts": "2.17.0",
    "standard": "^11.0.1",
    "typescript": "^3.0.1"
  },
  "lint-staged": {
    "linters": {
      "src/**/*.tsx?": [
        "prettier-standard",
        "git add"
      ]
    }
  },
  "dependencies": {
    "lodash": "^4.17.10"
  }
}
